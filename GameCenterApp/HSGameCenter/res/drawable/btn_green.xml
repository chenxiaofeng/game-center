<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--<item android:drawable="@drawable/btn_green_normal" android:state_pressed="false" android:state_enabled="true"/>-->
    <!--<item android:drawable="@drawable/btn_green_pressed" android:state_pressed="true" android:state_enabled="true"/>-->
    <!--<item android:drawable="@drawable/btn_green_disabled" android:state_pressed="false" android:state_enabled="false"/>-->


    <item android:state_pressed="true" android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/radius_btn"/>
            <solid android:color="@color/color_green_pressed" />
        </shape>
    </item>
    <item android:state_pressed="false" android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/radius_btn"/>
            <solid android:color="@color/color_green_disable" />
        </shape>
    </item>
    <item android:state_pressed="false" android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/radius_btn"/>
            <solid android:color="@color/color_green_normal" />
        </shape>
    </item>
</selector>
